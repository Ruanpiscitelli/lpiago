{
  "name": "aviator-app",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run clean-ports && astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:prod": "NODE_ENV=production astro build",
    "preview": "npm run clean-ports && astro preview",
    "astro": "astro",
    "lint": "astro check",
    "clean": "rm -rf dist/",
    "prebuild:prod": "npm run clean",
    "clean-ports": "kill $(lsof -t -i:4321-4330) 2>/dev/null || true"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "astro": "^4.15.11",
    "lightningcss": "^1.29.1",
    "sharp": "^0.33.5",
    "@rollup/wasm-node": "^4.9.0",
    "rollup": "npm:@rollup/wasm-node"
  },
  "devDependencies": {
    "astro-compress": "^2.3.6",
    "terser": "^5.39.0",
    "typescript": "^5.8.2",
    "vite-plugin-compression": "^0.5.1"
  }
}
